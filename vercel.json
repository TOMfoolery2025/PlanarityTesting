{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    
    {
      "src": "/(script\\.js|favicon\\.ico)",
      "dest": "/$1"
    },

    {
      "src": "/planarity",
      "dest": "api/index.py"
    }
  ]
}
